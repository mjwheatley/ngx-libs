{
  "name": "ngx-libs",
  "version": "1.0.0",
  "description": "Project for development of @mawhea angular library node modules",
  "homepage": "https://github.com/mjwheatley/ngx-libs",
  "license": "MIT",
  "author": "mjwheatley",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:env": ". environments/${AMPLIFY_ENV}.env.sh && node environment.js",
    "build:modules:ngx-core": "ng build @mawhea/ngx-core",
    "watch:ngx-core": "ng build @mawhea/ngx-core --watch",
    "publish:ngx-core": "npm run build:modules:ngx-core && cd dist/mawhea/ngx-core && npm publish --access public",
    "build:modules:ngx-camera": "ng build @mawhea/ngx-camera",
    "watch:ngx-camera": "ng build @mawhea/ngx-camera --watch",
    "publish:ngx-camera": "npm run build:modules:ngx-camera && cd dist/mawhea/ngx-camera && npm publish --access public",
    "build:modules:ngx-amplify-auth-ui": "ng build @mawhea/ngx-amplify-auth-ui",
    "watch:ngx-amplify-auth-ui": "ng build @mawhea/ngx-amplify-auth-ui --watch",
    "publish:ngx-amplify-auth-ui": "npm run build:modules:ngx-amplify-auth-ui && cd dist/mawhea/ngx-amplify-auth-ui && npm publish --access public",
    "build:modules": "run-s build:modules:*"
  },
